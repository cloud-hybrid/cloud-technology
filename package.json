{
    "name": "cloud-technology",
    "version": "0.0.0",
    "private": true,
    "license": "BSD-3-Clause",
    "readme": "README.md",
    "repository": "git+https://github.com/cloud-hybrid/cloud-technology.git",
    "workspaces": [
        "packages/*",
        "documentation/*",
        "utilities/*"
    ],
    "scripts": {
        "start.ui-template": "cd ./packages/ui-template && npm run start",
        "build.ui-template": "cd ./packages/ui-template && npm run build",
        "install.ui-template": "cd ./packages/ui-template && npm install .",
        "serve.ui-template": "cd ./packages/ui-template && serve -s ./build",
        "start.carbon-tutorial": "cd ./packages/carbon && npm run start",
        "build.carbon-tutorial": "cd ./packages/carbon && npm run build",
        "install.carbon-tutorial": "cd ./packages/carbon && npm install .",
        "serve.development.carbon-tutorial": "serve -s ./packages/carbon/build"
    },
    "bundledDependencies": [
        "workspaces:packages/cli",
        "workspaces:packages/github-api",
        "workspaces:packages/parser",
        "workspaces:packages/subprocess-handler",
        "workspaces:packages/systems-manager",
        "workspaces:packages/ui-template",
        "workspaces:packages/carbon"
    ]
}

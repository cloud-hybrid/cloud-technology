{
    "name": "cloud-technology",
    "version": "0.0.0",
    "private": true,
    "license": "BSD-3-Clause",
    "readme": "README.md",
    "engines": { "node": "^16.x" },
    "scripts": {
        "start.ui-template": "cd ./packages/ui-template && npm run start",
        "build.ui-template": "cd ./packages/ui-template && npm run build",
        "install.ui-template": "cd ./packages/ui-template && npm install .",
        "serve.ui-template": "cd ./packages/ui-template && serve -s ./build"
    },
    "repository": "git+https://github.com/cloud-hybrid/cloud-technology.git",
    "workspaces": [
        "library/*",
        "packages/*",
        "documentation/*",
        "utilities/*"
    ],
    "bundledDependencies": [
        "workspaces:packages/*"
    ]
}
